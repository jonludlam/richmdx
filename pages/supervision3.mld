{@ocaml[
let remove_top = 
  let rec loop = fun l r ->
    match l with
    | Lf -> Lf
    | Br (x, ll, lr) ->
      Br (x, r, loop ll lr)
  in
  function
  | Lf -> Lf
  | Br (_, ll, lr) -> loop ll lr

let t =
  let list = List.init 16 (fun i -> i+1) in
  List.fold_left (fun t i -> update (t, i, i)) Lf list;;

let _ =
  printer string_of_int (remove_top t);;
